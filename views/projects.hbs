<div class="my-5">
    <h2>My Projects</h2>
    {{#each projects}}
        <div class="row m-3 p-3 shadow align-items-center">
            <div class="col-2">
                <img src="/images/{{this.image}}" alt="">
            </div>
            <div class="col"><p>
                {{this.title}}
            </p></div>
        </div>
    {{/each}}
</div>